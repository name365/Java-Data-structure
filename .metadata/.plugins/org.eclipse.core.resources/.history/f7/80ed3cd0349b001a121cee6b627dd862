package github;

public class SparceArray {

	public static void main(String[] args) {
		
		//创建一个原始的二维数组 11*11
		// 0:表示没有棋子,1表示黑子,2表示白子
		int chessArr[][] = new int[11][11];
		chessArr[1][3] = 1;
		chessArr[2][7] = 2;
		//输出原始二维数组
		for(int[] row : chessArr){
			for(int data : row){
				System.out.printf("%d\t",data);
			}
			System.out.println();
		}
	
	}

}
